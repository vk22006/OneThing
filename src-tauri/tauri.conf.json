{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "OneThing",
  "version": "0.1.0",
  "identifier": "com.kishore.onething",
  "build": {
    "frontendDist": "../build",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "OneThing",
        "width": 1400,
        "height": 800,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/logo_32x32.png",
      "icons/logo_128x128.png",
      "icons/logo_128x128@2x.png",
      "icons/logo.icns",
      "icons/logo.ico"
    ]
  }
}
